<?xml version="1.0" encoding="UTF-8"?>
<package xmlns="http://www.idpf.org/2007/opf" version="2.0">
  <metadata xmlns:dc="http://purl.org/dc/elements/1.1/">
    <dc:title>Ukrainian-English Dictionary</dc:title>
    <dc:language>ru</dc:language>
    <dc:creator>casancam (Wiktionary/DBnary)</dc:creator>
    <dc:publisher>Open Source</dc:publisher>
  </metadata>

  <x-metadata>
    <!-- The language of the dictionary's source words (configured as Russian for Kindle compatibility) -->
    <DictionaryInLanguage>ru</DictionaryInLanguage>

    <!-- The language of the dictionary's definitions (English) -->
    <DictionaryOutLanguage>en</DictionaryOutLanguage>

    <DefaultLookupIndex>default</DefaultLookupIndex>

    <!-- Explicitly marks this as a dictionary -->
    <CdeType>DICTIONARY</CdeType>

    <!-- UTF-8 encoding for Cyrillic compatibility -->
    <output encoding="UTF-8" />

    <!-- Human-readable title -->
    <Title>Ukrainian-English Dictionary</Title>
  </x-metadata>

  <manifest>
    <!-- Main dictionary files -->
    <item id="dictionary1" href="dictionary-1.xhtml" media-type="application/xhtml+xml" />
    <item id="dictionary2" href="dictionary-2.xhtml" media-type="application/xhtml+xml" />
    <item id="dictionary3" href="dictionary-3.xhtml" media-type="application/xhtml+xml" />
  </manifest>

  <spine>
    <itemref idref="dictionary1" />
    <itemref idref="dictionary2" />
    <itemref idref="dictionary3" />
  </spine>

  <guide>
    <reference type="index" title="Dictionary Index" href="dictionary-1.xhtml" />
  </guide>
</package>
